		{
	"manifest_version": 2,
	"name": "Regular Expression User Agent Switcher",
	"description": "Switches user agent to specified one based upon URL rules",
	"version": "1.0",
	"browser_action":{
		"default_icon": "0.png",
		"default_title": "User Agent Switcher",
		"default_popup": "overview.html"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"storage",
		"tabs",
		"<all_urls>"
	],
	"background": {
		"scripts": ["switchuseragent.js"],
		"persistent": true
	}
}
